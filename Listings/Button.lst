N((__CHANNEL__) == LL_TIM_CHANNEL_CH1N) ? 1U :\
N((__CHANNEL__) == LL_TIM_CHANNEL_CH2) ? 2U :\
N((__CHANNEL__) == LL_TIM_CHANNEL_CH2N) ? 3U :\
N((__CHANNEL__) == LL_TIM_CHANNEL_CH3) ? 4U :\
N((__CHANNEL__) == LL_TIM_CHANNEL_CH3N) ? 5U :\

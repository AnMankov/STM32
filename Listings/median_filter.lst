Nclass TMedianFilter
N{
Npublic:
N  struct TPair
N  {
N    struct TPair *Ptr;
N    int16_t       Val;
N  };
N
Npublic:
N  TMedianFilter( int16_t _Stopper );
N  ~TMedianFilter();
N  
N  int16_t process( int16_t Val );
Nprotected:
Nprivate:
N  static constexpr uint8_t Size = 5;
N
N  TPair Buf[ Size ];
N  TPair *DataPtr;
N  TPair  Small;
N  TPair  Big;
N  TPair *Successor;
N  TPair *Scan;
N  TPair *Scanold;
N  TPair *Median;
N  
N  int16_t Stopper;
N};
N
N#endif //__MEDIAN_FILTER_H

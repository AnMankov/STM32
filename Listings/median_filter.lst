N
NTMedianFilter::TMedianFilter( int16_t _Stopper )
N:
NDataPtr( Buf ),
NSmall( { nullptr, _Stopper } ),
NBig( { &Small, 0 } ),
NStopper( _Stopper )
N{
N
N}
N
NTMedianFilter::~TMedianFilter()
N{
N
N}
N
Nint16_t TMedianFilter::process( int16_t Val )
N{                                                      
N  uint16_t i;                                                        
N                                                                     
N  if ( Val == Stopper )                                            
N  {                                                                  
N    Val = Stopper + 1;                                             /* No Stoppers allowed. */
N  }                                                                  
N                                                                     
N  if ( (++DataPtr - Buf) >= Size )                 
N  {                                                                  
N    DataPtr = Buf;                                                 /* Increment and wrap data in Ptrer.*/
N  }                                                                  
N                                                                     
N  DataPtr->Val = Val;                                              /* Copy in new Val */
N  Successor = DataPtr->Ptr;                                        /* Save Ptrer to old Val's Successor */
N  Median = &Big;                                                   /* Median initially to first in chain */
N  Scanold = nullptr;                                               /* Scanold initially nullptr. */
N  Scan = &Big;                                                     /* Ptrs to Ptrer to first (largest) Val in chain */
N                                                                     
N                                                                     
N  if ( Scan->Ptr == DataPtr )                                     
N  {                                                                  
N   Scan->Ptr = Successor;                                          /* Handle chain-out of first item in chain as special case */
N  }                                                                  
N													                  
N  Scanold = Scan;                                                  /* Save this Ptrer and */
N  Scan = Scan->Ptr ;                                               /* step down chain */
N                                                                     
N  for ( i = 0 ; i < Size; ++i )                                    /* Loop through the chain, normal loop exit via break. */
N  {                                                                
N    /* Handle odd-numbered item in chain */                        
N    if ( Scan->Ptr == DataPtr )                                 
N    {                                                              
N      Scan->Ptr = Successor;                                       /* Chain out the old Val.*/
N    }                                                                
N                                                                     
N    if ( Scan->Val < Val )                                         /* If Val is larger than Scanned Val,*/
N    {                                                                 
N      DataPtr->Ptr = Scanold->Ptr;                                 /* Chain it in here. */
N      Scanold->Ptr = DataPtr;                                      /* Mark it chained in. */
N      Val = Stopper;
N    };
N    
N    /* Step Median Ptrer down chain after doing odd-numbered element */
N    Median = Median->Ptr;                                          /* Step Median Ptrer. */
N    if ( Scan == &Small )                                            
N    {                                                                
N      break;                                                       /* Break at end of chain */
N    }                                                                
N	                                                                 
N    Scanold = Scan;                                                /* Save this Ptrer and */
N    Scan = Scan->Ptr;                                              /* step down chain */
N    
N    /* Handle even-numbered item in chain. */
N    if ( Scan->Ptr == DataPtr )
N    {
N      Scan->Ptr = Successor;
N    }
N    
N    if ( Scan->Val < Val )
N    {
N      DataPtr->Ptr = Scanold->Ptr;
N      Scanold->Ptr = DataPtr;
N      Val = Stopper;
N    }
N    
N    if ( Scan == &Small )
N    {
N      break;
N    }
N    
N    Scanold = Scan;
N    Scan = Scan->Ptr;
N  }
N 
N  return Median->Val;
N}
